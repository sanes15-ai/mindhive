{
  "name": "mindhive",
  "version": "1.0.0",
  "description": "MindHive - The World's First Collective Code Intelligence Network",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "validate": "tsx scripts/validate-env.ts",
    "setup": "npm install && npm run db:generate && tsx scripts/validate-env.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "ai",
    "collective-intelligence",
    "code-generation",
    "self-healing",
    "developer-tools",
    "multi-ai",
    "nexus",
    "time-travel-debugging",
    "anti-hallucination"
  ],
  "author": "Abdur Rehman <contact@github.com> (https://github.com/sanes15-ai)",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanes15-ai/mindhive.git"
  },
  "bugs": {
    "url": "https://github.com/sanes15-ai/mindhive/issues"
  },
  "homepage": "https://github.com/sanes15-ai/mindhive#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@google/generative-ai": "^0.15.0",
    "@pinecone-database/pinecone": "^2.2.0",
    "@prisma/client": "^5.22.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.8.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.2.6",
    "morgan": "^1.10.0",
    "ollama": "^0.5.0",
    "openai": "^4.52.0",
    "socket.io": "^4.7.5",
    "winston": "^3.13.0",
    "zod": "^3.23.8",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.5",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.0",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
